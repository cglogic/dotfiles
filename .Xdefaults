URxvt*termName: rxvt-unicode-256color
URxvt*internalBorder: 3
URxvt*externalBorder: 0
URxvt*scrollBar: false
!URxvt*borderLess: true
URxvt*depth: 24
!URxvt*background: rgba:0000/0000/0000/c800
URxvt*buffered: true
!URxvt*font: xft:Hack:antialias=true:autohinting=true:size=9,xft:M+ 2m:antialias=true:autohinting=true:size=9
URxvt*font: xft:FNAME:size=FSIZE:antialias=true:autohinting=true
!URxvt*font: xft:FNAME:pixelsize=25:antialias=true:autohinting=true
URxvt*boldFont:
URxvt*italicFont:
URxvt*boldItalicFont:
URxvt*skipBuiltinGlyphs: true
URxvt*lineSpace: 0
URxvt*letterSpace: 0
URxvt*saveLines: 0
URxvt*secondaryScreen: true
URxvt*secondaryScroll: false
URxvt*jumpScroll: true
URxvt*skipScroll: true
URxvt*scrollWithBuffer: false
URxvt*scrollTtyKeypress: true
URxvt*scrollTtyOutput: false
URxvt*loginShell: true
URxvt*perl-ext:
URxvt*perl-ext-common:
!URxvt*intensityStyles: false

! Fake transparency (copies the wallpaper to the background of the terminal)
!urxvt*transparent: true
!urxvt*shading: 30

!Xft*dpi: 96
Xft*antialias: true
!Xft*rgba: rgb
Xft*hinting: true
Xft*lcdfilter: lcddefault
Xft*hintstyle: hintslight

XTerm*termName: xterm-256color
XTerm*locale: true
XTerm*metaSendsEscape: true
XTerm*saveLines: 0
XTerm*scrollBar: false
XTerm*renderFont: true
!XTerm*faceName: FNAME:size=FSIZE:antialias=true:autohinting=true
XTerm*faceName: FNAME
XTerm*faceSize: FSIZE
XTerm*allowBoldFonts: false
XTerm*internalBorder: 3
XTerm*selectToClipboard: true
XTerm*fullscreen: never
!XTerm*charClass: 33:48,36-47:48,58-59:48,61:48,63-64:48,95:48,126:48
!XTerm*on3Clicks: regex [[:alpha:]]+://([[:alnum:]!#+,./=?@_~-]|(%[[:xdigit:]][[:xdigit:]]))+
!XTerm*buffered: true
!XTerm*bufferedFPS: 60

dzen2.font: FNAME:size=FSIZE
dzen2.foreground: #e0e0e0
dzen2.background: #005577

xnotify.title.font: xft:FNAME:size=FSIZE:antialias=true:autohinting=true
xnotify.body.font: xft:FNAME:size=FSIZE:antialias=true:autohinting=true
xnotify.background: #131313
xnotify.foreground: #e0e0e0
xnotify.border: #268bd2
xnotify.borderWidth: 2
xnotify.leading: 2
xnotify.padding: 2
xnotify.alignment: center

rofi.dpi: 0
rofi.font: FNAME FSIZE
rofi.scroll-method: false
rofi.hide-scrollbar: true
! Enable the extended coloring options
rofi.color-enabled: true
!                  bg       border   separator
rofi.color-window: #131313, #268bd2, #268bd2
!                  bg       fg       bg-alt   hl-bg    hl-fg
rofi.color-normal: #292929, #e0e0e0, #191919, #268bd2, #ffffff
rofi.color-active: #292929, #46abf2, #292929, #268bd2, #ffffff
rofi.color-urgent: #292929, #f3843d, #292929, #268bd2, #ffffff
rofi.separator-style: solid

*.foreground:   #d0d0d0
*.background:   #131313
*.cursorColor:  #d0d0d0

