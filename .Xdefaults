URxvt*termName: rxvt-unicode-256color
URxvt*internalBorder: 0
URxvt*externalBorder: 0
URxvt*scrollBar: false
!URxvt*borderLess: true
URxvt*depth: 32
!URxvt*background: rgba:0000/0000/0000/c800
URxvt*buffered: true
URxvt*font: xft:Hack:antialias=true:autohinting=true:size=9,xft:M+ 2m:antialias=true:autohinting=true:size=9
URxvt*boldFont: xft:Hack:antialias=true:autohinting=true:size=9,xft:M+ 2m:antialias=true:autohinting=true:size=9
URxvt*lineSpace: 0
URxvt*letterSpace: 0
URxvt*saveLines: 0
URxvt*secondaryScreen: true
URxvt*secondaryScroll: false
URxvt*jumpScroll: true
URxvt*skipScroll: true
URxvt*scrollWithBuffer: false
URxvt*scrollTtyKeypress: true
URxvt*scrollTtyOutput: false
URxvt*loginShell: true
URxvt*perl-ext:
URxvt*perl-ext-common:
!URxvt*intensityStyles: false

! Fake transparency (copies the wallpaper to the background of the terminal)
!urxvt*transparent: true
!urxvt*shading: 30

!Xft*dpi: 96
Xft*antialias: true
!Xft*rgba: rgb
Xft*hinting: true
Xft*lcdfilter: lcddefault
Xft*hintstyle: hintslight

XTerm*renderFont: true
XTerm*faceName: Hack
XTerm*faceSize: 14

#include ".xcolors"

Xcursor*theme: OpenZone_Ice
