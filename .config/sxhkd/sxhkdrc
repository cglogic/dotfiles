super + r
	~/.bin/srofi -show run

super + w
	~/.bin/srofi -show window

super + Tab
	bspc node focused -f next.local

super + shift + Tab
	bspc node focused -f prev.local

super + {j,k}
	bspc node @/ -R {90,-90}

super + c
	bspc node focused --close

super + x
	bspc node focused --kill

super + q
	bspc quit

super + {s,f}
	bspc desktop -l {tiled,monocle}

super + bracket{left,right}
	bspc desktop -f {prev,next}

super + {1-9}
	bspc desktop -f ^{1-9}

super + Escape
	pkill -USR1 -x sxhkd

super + Return
	urxvt -e tcsh

super + l
	i3lock -c 000000 -f

super + grave
	~/.bin/sdmenu

super + BackSpace
	~/.bin/stmux

super + b
	~/.bin/sfirefox

super + g
	~/.bin/schrome

super + v
	~/.bin/smonitor

super + d
	~/.bin/stouchpad

super + p
	printscreen

super + h
	~/.bin/swall

super + t
	~/.bin/stelegram

super + z
	~/.bin/crt

super + ,
	xset dpms force off

super + .
	xset dpms force on

super + F10
	xset s on +dpms

super + F11
	xset s off -dpms

super + @F12
	xset dpms force off



alt + r
	~/bin/srofi -show run

alt + w
	~/bin/srofi -show window

alt + Tab
	bspc node focused -f next.local

alt + shift + Tab
	bspc node focused -f prev.local

alt + {j,k}
	bspc node @/ -R {90,-90}

alt + c
	bspc node focused --close

alt + x
	bspc node focused --kill

alt + q
	bspc quit

alt + {s,f}
	bspc desktop -l {tiled,monocle}

alt + bracket{left,right}
	bspc desktop -f {prev,next}

alt + {1-9}
	bspc desktop -f ^{1-9}

alt + Escape
	pkill -USR1 -x sxhkd

alt + Return
	urxvt -e tcsh

alt + l
	i3lock -c 000000 -f

