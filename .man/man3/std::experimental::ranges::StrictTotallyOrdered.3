   template <class T>

   concept bool StrictTotallyOrdered() {
   return ranges::EqualityComparable<T>() &&
   requires(const T a, const T b) {
   { a < b } -> ranges::Boolean;                           \fB(1)\fP (ranges TS)
   { a > b } -> ranges::Boolean;
   { a <= b } -> ranges::Boolean;
   { a >= b } -> ranges::Boolean;
   };

   }
